<?xml version="1.0" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           targetNamespace="https://www.abc.com"
           xmlns="https://www.abc.com"
           elementFormDefault="qualified">
    <xs:complexType name="Books">
        <xs:simpleContent>
                <xs:extension base="xs:string">
                    <xs:attribute name="BookCode" type="xs:positiveInteger"></xs:attribute>
                </xs:extension>
        </xs:simpleContent>
    </xs:complexType>
</xs:schema>